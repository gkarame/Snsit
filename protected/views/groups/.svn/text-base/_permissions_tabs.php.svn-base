<div class="p_tab">	<?php 		if(isset($menu) && !empty($menu))		{			foreach((array)$menu as $name => $item)			{				// If parent is checked or has no parent return 1 else 0 				$parent_read = (($tab == 'general') || (isset($user_permissions['general-'.$tab]['read']) && $user_permissions['general-'.$tab]['read'] == 1)) ? 1 : 0;				// if Parent write is checked return 1 else 0				$parent_write = (($tab == 'general') || (isset($user_permissions['general-'.$tab]['write']) && $user_permissions['general-'.$tab]['write'] == 1)) ? 1 : 0;								// If parent is not checked Disable field				$disabled_read = ($parent_read) ? '' : 'disabled="disabled"';				// disable checkbox if parent is not checked or parent is checked but the read option is not checked				$disabled_write = ($parent_write) ? '' : 'disabled="disabled"';								// Check if it is marked to be checked				$checked_write = ($disabled_write == '' && (isset($user_permissions[$tab.'-'.$name]['write']) && $user_permissions[$tab.'-'.$name]['write'] == 1)) ? 'checked=""checked"' : '';				// If field is 1 check 				$checked_read = ($disabled_read == '' && (isset($user_permissions[$tab.'-'.$name]['read']) && $user_permissions[$tab.'-'.$name]['read'] == 1)) ? 'checked=""checked"' : '';								//special check				$checked_read_submitted = ($disabled_read == '' && (isset($user_permissions[$tab.'-'.$name]['read'][0]) && $user_permissions[$tab.'-'.$name]['read'][0] == 1)) ? 'checked=""checked"' : '';				$checked_read_approved = ($disabled_read == '' && (isset($user_permissions[$tab.'-'.$name]['read'][1]) && $user_permissions[$tab.'-'.$name]['read'][1] == 1)) ? 'checked=""checked"' : '';				$checked_read_paid = ($disabled_read == '' && (isset($user_permissions[$tab.'-'.$name]['read'][2]) && $user_permissions[$tab.'-'.$name]['read'][2] == 1)) ? 'checked=""checked"' : '';		?>		<?php 		if('expenses_approval' == $name) {			?>			<div class="permission_row name_<?php echo $tab ?>">				<div class="ppage" style="width:200px"><span></span> <?php echo ($tab == 'general') ? ucFirst($name) : $item['label']; ?></div>				<div class="chk read" style="width:160px" onclick="CheckOrUncheckInput(this)">					<div style="margin-left:10px" class="input<?php echo ($disabled_read) ? ' checkboxDisabled' : '';?>"></div>					<input type="hidden" value="0"  name="<?php echo  $tab . '-' . $name; ?>[read][0]" <?php echo $disabled_read;?> <?php echo $checked_read_submitted;?>>					<input type="checkbox" class="parent_<?php echo $tab ?>" name="<?php echo  $tab . '-' . $name; ?>[read][0]" value="1" <?php echo $disabled_read;?> <?php echo $checked_read_submitted;?>/>					<?php echo Yii::t('translations', 'Read Submitted');?>				</div>				<div class="chk read" style="width:160px" onclick="CheckOrUncheckInput(this)">					<div style="margin-left:10px" class="input<?php echo ($disabled_read) ? ' checkboxDisabled' : '';?>"></div>					<input type="hidden" value="0"  name="<?php echo  $tab . '-' . $name; ?>[read][1]" <?php echo $disabled_read;?> <?php echo $checked_read_approved;?>>					<input type="checkbox" class="parent_<?php echo $tab ?>" name="<?php echo  $tab . '-' . $name; ?>[read][1]" value="1" <?php echo $disabled_read;?> <?php echo $checked_read_approved;?>/>					<?php echo Yii::t('translations', 'Read Approved');?>				</div>				<div class="chk read" style="width:160px" onclick="CheckOrUncheckInput(this)">					<div style="margin-left:10px" class="input<?php echo ($disabled_read) ? ' checkboxDisabled' : '';?>"></div>					<input type="hidden" value="0"  name="<?php echo  $tab . '-' . $name; ?>[read][2]" <?php echo $disabled_read;?> <?php echo $checked_read_paid;?>>					<input type="checkbox" class="parent_<?php echo $tab ?>" name="<?php echo  $tab . '-' . $name; ?>[read][2]" value="1" <?php echo $disabled_read;?> <?php echo $checked_read_paid;?>/>					<?php echo Yii::t('translations', 'Read Paid');?>				</div>								<div class="chk write" style="width:160px" onclick="CheckOrUncheckInput(this)">					<div style="margin-left:10px" class="input<?php echo ($disabled_write) ? ' checkboxDisabled' : '';?>"></div>					<input type="checkbox" class="parent_<?php echo $tab ?>" name="<?php echo $tab . '-' . $name; ?>[write]" value="2"  <?php echo $disabled_write;?> <?php echo $checked_write;?>/>					<?php echo Yii::t('translations', 'Write');?>				</div>			</div>			<?php		}		elseif(in_array($tab,array('alerts','quicklinks')))		{			?>	<div class="permission_row name_<?php echo $tab ?>">					<div class="ppage"><span></span> <?php echo ($tab == 'general') ? ucFirst($name) : $item['label']; ?></div>									<div class="chk read" onclick="CheckOrUncheckInput(this)">										<div class="input<?php echo ($disabled_read) ? ' checkboxDisabled' : '';?>"></div>										<input type="checkbox" class="parent_<?php echo $tab ?>" name="<?php echo  $tab . '-' . $name; ?>[read]" value="1" <?php echo $disabled_read;?> <?php echo $checked_read;?>/>										<?php echo Yii::t('translations', 'Read');?>									</div>																	</div>				<?php 			}		else		{		 ?>			<div class="permission_row name_<?php echo $tab ?>">				<div class="ppage"><span></span> <?php echo ($tab == 'general') ? ucFirst($name) : $item['label']; ?></div>				<div class="chk read" onclick="CheckOrUncheckInput(this)">					<div class="input<?php echo ($disabled_read) ? ' checkboxDisabled' : '';?>"></div>					<input type="checkbox" class="parent_<?php echo $tab ?>" name="<?php echo  $tab . '-' . $name; ?>[read]" value="1" <?php echo $disabled_read;?> <?php echo $checked_read;?>/>					<?php echo Yii::t('translations', 'Read');?>				</div>				<div class="chk write" onclick="CheckOrUncheckInput(this)">					<div class="input<?php echo ($disabled_write) ? ' checkboxDisabled' : '';?>"></div>					<input type="checkbox" class="parent_<?php echo $tab ?>" name="<?php echo $tab . '-' . $name; ?>[write]" value="2"  <?php echo $disabled_write;?> <?php echo $checked_write;?>/>					<?php echo Yii::t('translations', 'Write');?>				</div>			</div>	<?php 				}			}		}	?></div><!-- form -->
	text-decoration: underline;
}

.items thead tr th {
	font: bold 12px Arial!important;
	text-transform: uppercase;
	color: #1f1f1f!important;
	background: #f0f0f0!important;
	border: 0!important;
	/*background: url(../images/up_down.png) no-repeat left center!important;*/
	cursor: pointer;
	text-align:left !important;
}

.items thead tr th a {
	font: bold 12px Arial!important;
	text-transform: uppercase;
	color: #1f1f1f!important;
	border: 0!important;
	cursor: pointer;
	background: url(../images/up_down.png) no-repeat 0px center;
	padding-left: 12px;
}

.grid-view table.items th a.asc {
	background: url(../images/asc.png) no-repeat 0px center!important;
}

.grid-view table.items th a.desc {
	background: url(../images/desc.png) no-repeat 0px center!important;
}


	
